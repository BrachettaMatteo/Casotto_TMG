            text="Prenota Comanda"/>
    <Label fx:id="lbDettagliComanda" layoutX="14.0" layoutY="221.0" prefHeight="155.0" prefWidth="289.0"
           textAlignment="CENTER" wrapText="true"/>
    <ComboBox fx:id="comboNomeProdotto" layoutX="84.0" layoutY="27.0" onAction="#setCosto" prefWidth="150.0"
              promptText="nome prodotto"/>
    <Spinner fx:id="spQuantita" layoutX="257.0" layoutY="28.0" prefHeight="26.0" prefWidth="87.0"/>
    <Button fx:id="btnAggiungi" layoutX="364.0" layoutY="28.0" mnemonicParsing="false" onAction="#aggiungiProdotto"
            text="aggiungiProdotto"/>
    <Label layoutX="84.0" layoutY="59.0" text="Costo per articolo:"/>
    <Label fx:id="lbcosto" layoutX="191.0" layoutY="59.0"/>

</AnchorPane>
